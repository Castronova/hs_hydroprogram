        <dc:type rdf:resource="http://hydroshare.org/terms/timeSeries"/>
        <!-- The description element has a single sub-element called abstract -->
        <dc:description>
            <rdf:Description>
                <dcterms:abstract>
		 			This dataset contains water temperature values recorded every 30 minutes in the Little Bear River at Mendon Road near Mendon, UT, USA. 
		 			The values were recorded using a Hydrolab Minisonde 5 multiparameter water quality sonde connected to a Campbell Scientific datalogger. 
		 			Recorded values represent the average water temperature over each 30 minute interval with values recorded at the end of the interval.
                </dcterms:abstract>
            </rdf:Description>
        </dc:description>
